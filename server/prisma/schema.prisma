// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model DiscardPoint {
  id            String  @id @default(uuid())
  owner         String
  discardObject String
  latitude      Decimal
  longitude     Decimal

  @@map("discardPoint")
}
